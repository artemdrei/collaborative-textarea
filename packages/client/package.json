  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@teamwork/websocket-json-stream": "^2.0.0",
    "axios": "^0.21.0",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "reconnecting-websocket": "^4.2.0",
    "sharedb": "^1.0.0-beta",
    "sharedb-string-binding": "^1.0.0",
    "socket.io-client": "^4.0.0",
    "ws": "^7.2.0"
  }
}
