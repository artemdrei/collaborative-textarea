{
  "name": "@collaborative-textarea/client",
  "version": "1.0.0",
  "license": "MIT",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --watch",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "lint": "eslint --fix '*/**/*.{js,ts,tsx}'",
    "ts:watch": "tsc --skipLibCheck --allowJs --watch",
    "test": "jest",
    "test:watch": "jest --watch --verbose false",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json",
    "svg:optimize": "find ./src/assets/icons -type f -name '*.svg' -print0 | xargs -0 -n 1 -P 6 svgo",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*/**/*.{js,ts,tsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/regenerator-runtime/runtime"
    ],
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/__mocks__/svgMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/__mocks__/styleMock.js",
      "^@root/(.*)": "<rootDir>/src/$1",
      "^@api/(.*)": "<rootDir>/src/api/$1",
      "^@store/(.*)": "<rootDir>/src/store/$1"
    }
  },
  "browserslist": "> 1%, not dead",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artemdrei/Webpack.git"
  },
  "bugs": {
    "url": "https://github.com/artemdrei/Webpack/issues"
  },
  "homepage": "https://github.com/artemdrei/Webpack#readme",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.16",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.2",
    "@types/sharedb": "^1.0.14",
    "@types/socket.io-client": "^1.4.36",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "install": "^0.13.0",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.2",
    "node-sass": "^5.0.0",
    "npm": "^6.14.9",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "svg-react-loader": "^0.4.6",
    "svgo": "^1.3.2",
    "terser-webpack-plugin": "^4.2.2",
